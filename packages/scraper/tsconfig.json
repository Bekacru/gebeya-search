{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "@gebeyasearch/tsconfig/base.json",
    "compilerOptions": {
        "isolatedModules": false,
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src*"]
        },
        "noUncheckedIndexedAccess": false,
        "tsBuildInfoFile": ".tsbuildinfo"
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
}
